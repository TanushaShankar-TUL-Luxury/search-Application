{
  "ci": {
    "collect": { "numberOfRuns": 1 },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "first-contentful-paint": [
          "error",
          { "maxNumericValue": 90000, "aggregationMethod": "optimistic" }
        ],
        "interactive": [
          "error",
          { "maxNumericValue": 50000, "aggregationMethod": "optimistic" }
        ]
      }
    }
  }
}
